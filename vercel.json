{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "dist/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/dist/assets/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "framework": null,
  "public": true
}
