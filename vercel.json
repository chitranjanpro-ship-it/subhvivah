{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/og-images/(.*)", "destination": "/og-images/$1" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "framework": null,
  "public": true
}
